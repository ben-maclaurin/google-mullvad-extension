{
  "manifest_version": 2,
  "name": "Google Search Redirect",
  "version": "1.0",
  "description": "Redirects all searches to Google.com",
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Google Search Redirect"
  }
}
